pipeline {
  agent any
    
  tools {MAVEN "node"}
    
  stages {
        
    stage('Cloning Git') {
      steps {
        git 'https://github.com/asquarezone/game-of-life.git'
      }
    }
        
    stage('packaging') {
      steps {
        sh label:'',script:''' mvn clean package '''
      }
    }
     
    stage('Test') {
      steps {
        junit 'gameoflife-web/target/surefire-reports/*.xml' 
      }
    }      
  }
}
